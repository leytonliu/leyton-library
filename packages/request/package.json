{
  "name": "@leyton/request",
  "version": "7.0.10",
  "private": false,
  "description": "A request library based on axios, for web and uni-app.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "vite": "^7.0.6",
    "vitest": "^2.1.2"
  }
}
